  "description": "Badge overview for badges dk",
  "scripts": {
    "start": "npm-run-all -p start:jekyll watch:js",
      "start:jekyll": "jekyll serve",
    "build": "npm-run-all build:*",
      "build:jekyll": "jekyll build",
      "build:js": "uglifyjs _site/js/raw-search.js _site/js/cookie.js -m -o js/output.js",
    "watch": "npm-run-all -p watch:*",
